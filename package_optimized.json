  "description": "Fibuca backend - optimized for low-RAM systems",
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "@supabase/supabase-js": "^2.57.4",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
