    "start:low-ram": "NODE_OPTIONS='--max-old-space-size=256' node index.js",
    "start:debug": "NODE_OPTIONS='--max-old-space-size=256' node --inspect=9229 index.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop fibuca-backend",
    "pm2:logs": "pm2 logs fibuca-backend",
